{
  "name": "canvas-as",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/webassembly-web-api": "^0.0.1",
    "assemblyscript": "assemblyscript/assemblyscript",
    "npm-run-all": "^4.1.5",
    "onchange": "^5.2.0",
    "parcel-bundler": "^1.10.3",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2"
  },
  "scripts": {
    "start": "run-s asbuild canvas-as:watch",
    "canvas-as:watch": "run-p parcel:serve asbuild:watch",
    "parcel:serve": "parcel src/index.html",
    "asbuild:watch": "onchange \"assembly/**/*.ts\" -- npm run asbuild",
    "asbuild:untouched": "asc assembly/index.ts -b build/untouched.wasm -t build/untouched.wat --sourceMap --validate --debug --use Math=JSMath",
    "asbuild:optimized": "asc assembly/index.ts -b build/optimized.wasm -t build/optimized.wat --sourceMap --validate --optimize --use Math=JSMath",
    "asbuild": "run-p asbuild:untouched asbuild:optimized"
  },
  "dependencies": {
    "regenerator-runtime": "^0.13.1"
  }
}
